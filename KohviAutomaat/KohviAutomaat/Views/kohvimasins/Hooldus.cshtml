@model IEnumerable<KohviAutomaat.Models.kohvimasin>

@{
    ViewBag.Title = "Hooldus";
}

<h2>Hooldus</h2>

<p>
    @Html.ActionLink("Lisa jook", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Jooginimi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Täitepakis)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Topsejuua)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Jooginimi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Täitepakis)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Topsejuua)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
